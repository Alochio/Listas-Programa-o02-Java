package principal;
import principal.Universidade;

public class Departamento {

	private String[] nome;
	private String universidadeAssociada;

	int teste = 0;
	Universidade universidade;
	
	public String getNome(int valor){
		return this.getNome(valor);
	}
	public void setNome(String[] nome){
		this.nome = nome;
	}
	
	public String getUniversidadeAssociada(){
		return this.universidadeAssociada;
	}
	public void setUniversidadeAssociada(String universidadeAssociada){
		this.universidadeAssociada = this.universidade.getNome();
	}
}