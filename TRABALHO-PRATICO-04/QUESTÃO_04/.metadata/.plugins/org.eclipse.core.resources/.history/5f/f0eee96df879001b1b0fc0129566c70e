package principal;

public class Main {
	public static void main(String[] args){
		
		Blog blog = new Blog();
		
		Postagem postagem01 = new Postagem();
		
		blog.adcionarPostagem(postagem01);
		
		blog.publicarPostagem(postagem01);
		
		blog.apagarPostagem(postagem01);
		
		blog.listarPostagensPublicadas();
		
		
		
		
		
		
	}

}
