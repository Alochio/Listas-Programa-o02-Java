package principal;
import java.util.Date;

public class Postagem {

	private Integer id;
	private String titulo;
	private String texto;
	private Date data;
	
	public Integer getId(){
		return this.id;
	}
	public String getTitulo(){
		return this.titulo;
	}
	public String getTexte(){
		return this.texto;
	}
	public Date getData(){
		return this.data;
	}
	
