package principal;

public class Universidade {
	
	private String nome, cidade, estado;
	
	public String getNome(){
		return this.nome;
	}
	public String getCidade(){
		return this.cidade;
	}
	public String getEstado(){
		return this.estado;
	}
	
	public Universidade(String nome, String cidade, String estado){
		if (nome != null)
			System.out.println("A pessoa já está associada a uma Universidade");
		else{
			this.nome = nome;
			this.cidade = cidade;
			this.estado = estado;
		}
	}	
}