package principal;

public class Universidade {
	
	private String nome, endereco;
	
	public String getNome(){
		return this.nome;
	}
	public String getEndereco(){
		return this.endereco;
	}
	
	public Universidade(String nome,String endereco){		
			this.nome = nome;
			this.endereco = endereco;
	}	
}