package principal;

public class Pessoa {
	
	private String nome;
	private String sobrenome;
	
	
	public String getNome(){
		return this.nome;
	}
	public String getSobrenome(){
		return this.sobrenome;
	}
	
	
	public Pessoa(String nome, String sobrenome){
		this.nome = nome;
		this.sobrenome = sobrenome;
		
	}
	
	
	
}
