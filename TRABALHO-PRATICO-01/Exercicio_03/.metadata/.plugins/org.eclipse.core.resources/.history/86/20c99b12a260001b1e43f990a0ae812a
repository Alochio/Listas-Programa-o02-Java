package principal;

public class Main {
	public static void main(String[] args){
	
	int n = 4; //valor que será atribuido a n
	System.out.println(f(n)); //imprimindo o retorno da função
	
	}
	public static int f(int n){ //funcão f
		
		if(n == 1){
			return 1;
		}
		if(n == 2){
			return 2;
		}
		return 2 * f(n-1) + 3 * f(n-2); //Recursividade
	}
}