package formasGeometricas;
import formasGeometricas.FormasGeometricas;

public class CriaFormas {
	
	private FormasGeometricas novaForma;
	
	public FormasGeometricas CriaForma(String forma){
		
		if(forma.equals("Quadrado"))
			return new Quadrado();
		else if(forma.equals("Retangulo"))
			return new Quadrado();
		
		
		
		return novaForma;
		
	}
	
	
}
