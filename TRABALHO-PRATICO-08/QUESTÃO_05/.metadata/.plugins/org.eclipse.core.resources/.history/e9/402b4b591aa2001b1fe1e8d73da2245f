package testaFabricaDeCarros;

import frabricaDeCarros.*;

public class Cliente {
	public static void main(String[] args){
		
		FabricaDeCarros fab01 = new FabricaFiat();
		FabricaDeCarros fab02 = new FabricaFord();
		
		CarroSedan car01 = fab01.criaCarroSedan();
		CarroPopular car02 = fab01.criaCarroPopular();
		
		CarroSedan car03 = fab02.criaCarroSedan();
		CarroPopular car04 = fab02.criaCarroPopular();
		
		car01.exibirInfoSedan();
		car02.exibirInfoPopular();
		car03.exibirInfoSedan();
		car04.exibirInfoPopular();
	}

}
