package principal;

import java.util.List;

public class adaptador extends SomadorExistente{
	
	private SomadorExistente objetoSomadorExistente;
	
	public adaptador(SomadorExistente somExistente){
		this.objetoSomadorExistente = somExistente;
	}
	
	@Override
	public int somaLista(List<Integer> lista){
		int resultado = 0;
		for(int i: lista) resultado += 1;
			return resultado;
	}
}
