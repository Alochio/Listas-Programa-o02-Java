	public static void main(String[] args){
		
		Gato gato1 = new Gato();
		
		gato1.setNome("Frajola");
		gato1.setRaca("Perça");
		
		System.out.println("Informações do gato: \n" + "Nome: " + gato1.getNome() + 
				"\nRaça: " + gato1.getRaca());
		
		gato1.Caminha();
		gato1.mia();
		
		System.out.println("-----------------------------------------------------");
		
		Cachorro dog1 = new Cachorro();
		
		dog1.setNome("Thor");
		dog1.setRaca("Labrador");
		
		
		
		
		
		
	}

}
