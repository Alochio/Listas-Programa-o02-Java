package principal;

public class Main {
	public static void main(String[] args){
		
		Lampada l1 = new Lampada();
		System.out.println(l1.instanciasCriadas);
		System.out.println(l1.getWhatsDaLampada());
		System.out.println(l1.Eeconomida(l1.getWhatsDaLampada()));
		
		Lampada l2 = new Lampada();
		System.out.println(l2.instanciasCriadas);
		System.out.println(l2.getWhatsDaLampada());
		System.out.println(l2.Eeconomida(l2.getWhatsDaLampada()));
		
		Lampada l3 = new Lampada(40);
		System.out.println(l3.instanciasCriadas);
		System.out.println(l3.getWhatsDaLampada());
		System.out.println(l3.Eeconomida(l3.getWhatsDaLampada()));
	
	
	
	}

}
